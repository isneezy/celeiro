{
  "name": "isneezy/celeiro",
  "description": "Helps you build your laravel app following repository pattern ",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Vilanculo",
      "email": "vilanculoivan@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "~5",
    "illuminate/support": "~5",
    "illuminate/pagination": "~5"
  },
  "autoload": {
    "psr-4": {
      "Isneezy\\Celeiro\\": "src",
      "Isneezy\\Celeiro\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "illuminate/http": "~5",
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^1.2"
  },

  "extra": {
    "laravel": {
      "providers" : [
        "Isneezy\\Celeiro\\Provider\\LaravelServiceProvider"
      ]
    }
  }
}
